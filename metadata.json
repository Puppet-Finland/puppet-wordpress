{
  "name": "bodgit-wordpress",
  "version": "1.0.2",
  "author": "bodgit",
  "summary": "Puppet Module for managing Wordpress",
  "license": "Apache-2.0",
  "source": "https://github.com/bodgit/puppet-wordpress",
